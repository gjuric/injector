{
    "name": "zalas/injector",
    "description": "Injects services from a PSR-11 service container into objects",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jakub Zalas",
            "email": "jakub@zalas.pl"
        }
    ],
    "require": {
        "php": "^7.1",
        "psr/container": "^1.0",
        "phpdocumentor/reflection-docblock": "^4.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Zalas\\Injector\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zalas\\Injector\\Tests\\": "tests/"
        }
    }
}
